        },
        "custom": {
            "fontWeight": {
                "thin": 100,
                "extra-light": 200,
                "light": 300,
                "regular": 400,
                "medium": 500,
                "semi-bold": 600,
                "bold": 700,
                "extra-bold": 800,
                "black": 900
            },
            "lineHeight": {
                "body": 1.5,
                "heading": 1.2,
                "one": 1
            }
        },
        "shadow": {
            "defaultPresets": false,
            "presets": [
                {
                    "name": "Form",
                    "shadow": "0 16px 40px 0 rgba(194, 194, 194, 0.20);",
                    "slug": "form"
                },
                {
                    "name": "Green",
                    "shadow": "1px 1px 16px 0px rgb(172, 237, 210)",
                    "slug": "green"
                },
                {
                    "name": "Purple",
                    "shadow": "1px 1px 16px 0px rgb(207, 192, 250)",
                    "slug": "purple"
                },
                {
                    "name": "Blue",
                    "shadow": "1px 1px 16px 0px rgb(183, 205, 255)",
                    "slug": "blue"
                },
                {
                    "name": "Yellow",
                    "shadow": "1px 1px 16px 0px rgb(255, 234, 187)",
                    "slug": "yellow"
                }
            ]
        },
        "typography": {
            "customFontSize": true,
            "dropCap": true,
            "writingMode": true,
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "lineHeight": true,
            "textColumns": true,
            "textDecoration": true,
            "textTransform": true,
            "fluid": {
                "minViewportWidth": "450px",
                "maxViewportWidth": "1440px"
            },
            "fontFamilies": [
                {
                    "fontFamily": "\"Inter\"",
                    "name": "Inter",
                    "slug": "inter",
                    "fontFace": [
                        {
                            "fontFamily": "\"Inter\"",
                            "fontDisplay": "swap",
                            "fontStyle": "normal",
                            "fontWeight": "100 700",
                            "src": [
                                "file:./assets/fonts/Inter/Inter-VariableFont.ttf"
                            ]
                        },
                        {
                            "fontFamily": "\"Inter\"",
                            "fontDisplay": "swap",
                            "fontStyle": "italic",
                            "fontWeight": "100 900",
                            "src": [
                                "file:./assets/fonts/Inter/Inter-Italic-VariableFont.ttf"
                            ]
                        }
                    ]
                },
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Libre Franklin, fontello, 'Inter', Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
                    "name": "System Font",
                    "slug": "system-font"
                }
            ],
            "fontSizes": [
                {
                    "fluid": false,
                    "name": "14px",
